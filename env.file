# URL to use in GitHub PRs link
JENKINS_URL=URL

# Admin credentials
JENKINS_ADMIN=user
JENKINS_PASSWORD=pass

# Token used to publish checks in GitHub
GITHUB_TOKEN=token

# Credentials used for publishing image on docker hub (not mandatory)
#DOCKER_USER=user
#DOCKER_PASSWORD=pass

# Ip that the jenkins-docker can reach to connect to the host docker by tls
DOCKER_HOST=ip.of.the.host
